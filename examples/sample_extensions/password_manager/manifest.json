{
	"manifest_version": 3,
	"name": "Sample Password Manager",
	"version": "1.0",
	"description": "A sample password manager extension for testing",
	"permissions": [
		"storage",
		"activeTab",
		"scripting"
	],
	"action": {
		"default_popup": "popup.html",
		"default_title": "Sample Password Manager"
	},
	"content_scripts": [{
		"matches": ["<all_urls>"],
		"js": ["content.js"]
	}]
}