{
	"manifest_version": 3,
	"name": "Sample Ad Blocker",
	"version": "1.0",
	"description": "A sample ad blocker extension for testing",
	"permissions": [
		"storage",
		"activeTab"
	],
	"action": {
		"default_popup": "popup.html",
		"default_title": "Sample Ad Blocker"
	},
	"content_scripts": [{
		"matches": ["<all_urls>"],
		"js": ["content.js"]
	}],
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png", 
		"128": "icon128.png"
	}
}